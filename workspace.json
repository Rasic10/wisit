{
  "version": 2,
  "projects": {
    "wisit.backend": {
      "root": "apps/wisit.backend",
      "sourceRoot": "apps/wisit.backend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/wisit.backend",
            "main": "apps/wisit.backend/src/main.ts",
            "tsConfig": "apps/wisit.backend/tsconfig.app.json",
            "assets": ["apps/wisit.backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/wisit.backend/src/environments/environment.ts",
                  "with": "apps/wisit.backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "wisit.backend:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/wisit.backend/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/wisit.backend"],
          "options": {
            "jestConfig": "apps/wisit.backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "wisit.mobile": {
      "root": "apps/wisit.mobile",
      "sourceRoot": "apps/wisit.mobile/src",
      "projectType": "application",
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start wisit.mobile"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": ["apps/wisit.mobile/build"],
          "options": {
            "entryFile": "apps/wisit.mobile/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/wisit.mobile/ios/main.jsbundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/wisit.mobile/android/app/build/outputs/bundle",
            "apps/wisit.mobile/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/wisit.mobile/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/wisit.mobile/android/main.jsbundle"
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "ensure-symlink": {
          "executor": "@nrwl/react-native:ensure-symlink",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/wisit.mobile/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/wisit.mobile"],
          "options": {
            "jestConfig": "apps/wisit.mobile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "wisit.mobile-e2e": {
      "root": "apps/wisit.mobile-e2e",
      "sourceRoot": "apps/wisit.mobile-e2e/src",
      "projectType": "application",
      "targets": {
        "build-ios": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "test-ios": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "build-android": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "test-android": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/wisit.mobile-e2e/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "wisit.backend"
}
